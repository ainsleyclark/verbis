{{ define "content" }}

    <div style="padding:100px;">
        <h1>Repeater</h1>
        <!-- Repeater -->
        {{ $repeater := repeater "repeater" }}
        {{ if $repeater.HasRows }}
            {{ range $row := $repeater}}
                {{  $row.SubField "text" }}

                <!-- Nested Repeater -->
                {{ $nested := $row.SubField "nested" }}
                {{ if $nested.HasRows }}
                    {{ range $nestedrow := $nested }}
                        {{ $nestedrow.SubField "text_test" }}
                    {{ end }}
               {{ end }}

            {{ end }}
        {{ end }}
    </div>

{{ end }}Ã¥